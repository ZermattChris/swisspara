{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": ".",
    "sourceMap": true,
    "inlineSourceMap": false,
    "paths": {
      "@src/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@images/*": ["src/images/*"],
      "@stores/*": ["src/components/stores/*"]
    }
  }
}